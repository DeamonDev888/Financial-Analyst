{
  "name": "novaquote-financial-analyst",
  "version": "1.0.0",
  "description": "Expert ES Futures Trading Agent with AI & Real-time Data Analysis",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "ts-node src/backend/server.ts",
    "test": "jest",
    "pipeline": "ts-node src/backend/scripts/run_news_data_pipeline.ts",
    "sentiment": "ts-node src/backend/scripts/run_agent_sentiment.ts",
    "sentiment:force": "ts-node -e \"import { SentimentAgent } from './src/backend/agents/SentimentAgent'; const agent = new SentimentAgent(); agent.analyzeMarketSentiment(true).then(console.log).finally(() => agent.cleanup());\"",
    "analyze": "ts-node src/backend/scripts/analyze_news_data.ts",
    "analyze:week": "npm run analyze week",
    "analyze:month": "npm run analyze month",
    "analyze:today": "npm run analyze today",
    "export:csv": "npm run analyze export",
    "data:dates": "npm run analyze dates",
    "data:today": "npm run analyze today",
    "refresh": "ts-node src/backend/scripts/refresh_news_cache.ts",
    "refresh:force": "npm run refresh --force",
    "refresh:cleanup": "npm run refresh --cleanup",
    "db:stats": "npm run refresh --stats",
    "db:init": "ts-node -e \"import { NewsDatabaseService } from './src/backend/database/NewsDatabaseService'; const db = new NewsDatabaseService(); db.testConnection().then(() => console.log('Database initialized')).finally(() => db.close());\""
  },
  "keywords": [
    "trading",
    "es-futures",
    "ai",
    "financial-analysis",
    "scraping"
  ],
  "author": "Deamon",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "ws": "^8.14.2",
    "zod": "^3.22.4",
    "puppeteer": "^21.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1"
  }
}

{
  "name": "novaquote-financial-analyst",
  "version": "1.0.0",
  "description": "Expert ES Futures Trading Agent with AI & Real-time Data Analysis",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "npx ts-node run.ts --analyze",
    "test": "jest",
    "analyze": "npx ts-node run.ts --analyze",
    "continuous": "npx ts-node run.ts --continuous",
    "status": "npx ts-node run.ts --status",
    "help": "npx ts-node run.ts --help",
    "pipeline": "ts-node src/backend/scripts/run_news_data_pipeline.ts",
    "sentiment": "ts-node src/backend/scripts/run_agent_sentiment.ts",
    "sentiment:force": "ts-node -e \"import { SentimentAgentFinal } from './src/backend/agents/SentimentAgentFinal'; const agent = new SentimentAgentFinal(); agent.analyzeMarketSentiment(true).then(console.log);\"",
    "analyze:week": "npm run analyze week",
    "analyze:month": "npm run analyze month",
    "analyze:today": "npm run analyze today",
    "export:csv": "npm run analyze export",
    "data:dates": "npm run analyze dates",
    "data:today": "npm run analyze today",
    "refresh": "ts-node src/backend/scripts/refresh_news_cache.ts",
    "refresh:force": "npm run refresh --force",
    "refresh:cleanup": "npm run refresh --cleanup",
    "db:stats": "npm run refresh --stats",
    "db:init": "npx ts-node create_database.ts",
    "vix": "ts-node src/backend/scripts/vix_simple_example.ts",
    "vix:live": "ts-node src/backend/scripts/get_vix_data.ts",
    "vix:dtc": "ts-node src/backend/scripts/vix_dtc_binary.ts",
    "vix:optimized": "ts-node src/backend/scripts/vix_dtc_optimized.ts",
    "vix:persistent": "ts-node src/backend/scripts/vix_persistent.ts",
    "vix:debug": "ts-node src/backend/scripts/vix_debug.ts",
    "vix:exchanges": "ts-node src/backend/scripts/vix_exchanges.ts",
    "vix:discovery": "ts-node src/backend/scripts/vix_discovery.ts",
    "vix:file": "ts-node src/backend/scripts/vix_sierra_file_reader.ts",
    "crypto:file": "ts-node src/backend/scripts/crypto_sierra_file_reader.ts"
  },
  "keywords": [
    "trading",
    "es-futures",
    "ai",
    "financial-analysis",
    "scraping"
  ],
  "author": "Deamon",
  "license": "ISC",
  "dependencies": {
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "playwright": "^1.56.1",
    "puppeteer": "^21.5.0",
    "sqlite3": "^5.1.7",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
